import socket

def test_connection(ip, port):
    try:
        s = socket.create_connection((ip, port), timeout=5)
        s.close()
        return True
    except Exception as e:
        print(f"Connection failed: {e}")
        return False

ip = "10.7.195.71"
port = 1433
if test_connection(ip, port):
    print("Connection successful")
else:
    print("Connection failed")
